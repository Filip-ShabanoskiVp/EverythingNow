@model IEnumerable<EverythingNow.Models.Tiket>
@{
    ViewData["Title"] = "Details";
}

<h1>Купени билети</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Sediste)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cena)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdNatprevar)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Sediste)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cena)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IdNatprevar)
                </td>
        </tr>
}
    </tbody>
</table>
<a asp-action="Index">Back</a>





